{
  "name": "reverser",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "dependencies": {
    "esbuild": "^0.14.3",
    "sass": "^1.44.0"
  },
  "scripts": {
    "build": "esbuild app/typescript/*.* --bundle --sourcemap --outdir=./public/assets/builds",
    "build:css": "sass ./app/stylesheet/application.sass.scss ./public/assets/builds/application.css --load-path=node_modules --embed-sources"
  },
  "version": "0.1.0"
}
